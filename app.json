{
  "expo": {
    "name": "Laps",
    "slug": "ski-platform",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "skiplatform",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ca.jessegross.skiplatform",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to verify you are moving during the game of S.K.I.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to display and upload images.",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to be able to call tricks hands-free.",
        "NSSpeechRecognitionUsageDescription": "Laps uses speech recognition to track your tricks hands-free.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "laps.api.jessegross.ca": {
              "NSAllowsArbitraryLoadsInWebContent": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png",
        "backgroundImage": "./assets/images/logo.png",
        "monochromeImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jesseg19.skiplatform",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Laps to use your location to track game distance."
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 33,
            "extraMavenRepos": [
              "../../node_modules/@notifee/react-native/android/libs"
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.789488486637-k5g7e0j9hssugsuu00l8q6vh8vhudeg4"
        }
      ],
      "expo-secure-store",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-video",
      [
        "expo-speech-recognition",
        {
          "microphonePermission": "Allow Laps to access the microphone",
          "speechRecognitionPermission": "Allow Laps to securely recognize user speech"
        }
      ],
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1bd3a793-f5d7-4f45-b7ba-fe43de60ddef"
      }
    }
  }
}